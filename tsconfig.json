{
  "compilerOptions": {
    "declaration": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "target": "es5",
    "jsx": "react",
    "resolveJsonModule": true
  },
  "include": ["./src/**/*.ts", "./src/**/*.tsx", "cypress", "./**/*.cy.tsx"]
}
